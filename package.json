{
  "name": "flat-obj",
  "version": "3.0.0",
  "repository": "lukeed/flat-obj",
  "description": "A tiny (188B) utility to flatten an object with customizable glue",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "flat.d.ts",
  "license": "MIT",
  "author": {
    "name": "Luke Edwards",
    "email": "luke.edwards05@gmail.com",
    "url": "https://lukeed.com"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
